{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 1,
    "quotes": 0,
    "no-unused-vars": 1,
    "camelcase": 1,
    "no-underscore-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": [0],
    "func-names": [0],
    "semi": [2, "never"],
    "no-extra-semi": [2],
    "space-before-function-paren": [2, "always"],
    "no-else-return": [0],
    "space-infix-ops": [0],
    "react/prefer-es6-class": [1],
    "react/prefer-stateless-function": [2],
  },
  "globals": {
    "__PREFIX_LINKS__": true,
  },
}
